<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane fx:id="pane" xmlns="http://javafx.com/javafx/8.0.172-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.Controller">
   <children>
      <ImageView fx:id="refrigeratorImg" fitHeight="150.0" fitWidth="200.0" layoutX="-12.0" layoutY="-34.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="/images/r.png" />
         </image>
      </ImageView>
      <Button fx:id="addBtn" mnemonicParsing="false" text="add" />
   </children>
</AnchorPane>
